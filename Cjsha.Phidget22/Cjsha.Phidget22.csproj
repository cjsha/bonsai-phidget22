<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Cjsha.Phidget22</Title>
    <Description>A Bonsai wrapper for Phidget22 API.</Description>
    <Authors>Cristian Sharp</Authors>
    <Copyright>Copyright ©  2025</Copyright>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIcon></PackageIcon>
    <PackageOutputPath></PackageOutputPath>
    <PackageTags>Bonsai Rx Cjsha.Phidget22</PackageTags>
    <TargetFramework>net472</TargetFramework>
    <Features>strict</Features>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.8.2" />
    <PackageReference Include="Phidget22.NET" Version="1.22.20250324" />
  </ItemGroup>

</Project>
